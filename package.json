{
  "name": "react-ssr-setup",
  "version": "2.1.0",
  "author": "Manuel Bieh <node@manuelbieh.de> (https://github.com/manuelbieh)",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0",
    "yarn": "^1.3.0"
  },
  "scripts": {
    "analyze": "yarn build:with-stats && yarn start:analyzer",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "build:with-stats": "cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > build/client/static/bundle-stats.json",
    "lint": "eslint ./src",
    "plop": "plop",
    "setup": "yarn link react react-dom",
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "start:analyzer": "webpack-bundle-analyzer build/client/static/bundle-stats.json",
    "test": "node scripts/test.js --env=jsdom",
    "test:update": "yarn test --updateSnapshot"
  },
  "lint-staged": {
    "*.{js,css}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint src --fix",
      "git add"
    ]
  },
  "moduleRoots": [
    "src/client",
    "src/server",
    "src/shared",
    "src",
    "node_modules"
  ],
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "connected-react-router": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-manifest-helpers": "^0.5.0",
    "history": "^4.7.2",
